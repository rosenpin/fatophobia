{
  "name": "fatophob-assessment",
  "version": "1.0.0",
  "description": "A web-based assessment tool for body image perception tendencies",
  "main": "index.html",
  "scripts": {
    "client:dev": "cd client && wrangler pages dev .",
    "client:deploy": "cd client && wrangler pages deploy .",
    "server:dev": "cd server && wrangler dev",
    "server:deploy": "cd server && wrangler deploy",
    "dev": "npm run client:dev",
    "deploy": "npm run client:deploy && npm run server:deploy"
  },
  "keywords": ["assessment", "body-image", "web-game", "cloudflare"],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "wrangler": "^3.0.0"
  }
}